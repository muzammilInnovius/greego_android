<?xml version="1.0" encoding="utf-8"?>
<LinearLayout xmlns:android="http://schemas.android.com/apk/res/android"
    android:id="@+id/row_rl1"
    android:layout_width="match_parent"
    android:layout_height="wrap_content"
    android:clickable="false"
    android:focusableInTouchMode="false"
    android:orientation="vertical">


    <RelativeLayout
        android:id="@+id/row_rl"
        android:layout_width="match_parent"
        android:layout_height="wrap_content"
        android:background="@color/white"
        android:focusableInTouchMode="false"
        android:padding="@dimen/scale_5dp">

        <ImageView
            android:id="@+id/imgVwLog"
            android:layout_width="@dimen/scale_35dp"
            android:layout_height="@dimen/scale_35dp"
            android:src="@mipmap/ic_launcher_round"
            android:visibility="gone" />

        <TextView
            android:id="@+id/listtextView"
            android:layout_width="wrap_content"
            android:layout_height="wrap_content"
            android:layout_centerVertical="true"
            android:layout_marginLeft="@dimen/scale_15dp"
            android:layout_toRightOf="@+id/imgVwLog"
            android:text="@string/title"
            android:textColor="@color/black"
            android:textSize="@dimen/txt_14"
            android:textStyle="bold" />


    </RelativeLayout>

    <!--<View-->
    <!--android:layout_width="match_parent"-->
    <!--android:layout_height="1dp"-->
    <!--android:background="@color/com_facebook_button_background_color_focused_disabled" />-->
</LinearLayout>